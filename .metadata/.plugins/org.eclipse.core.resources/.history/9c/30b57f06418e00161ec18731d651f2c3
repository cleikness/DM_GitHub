public class Week {
	Match[] matches;
	public Week(Match[] matches) {
		this.matches = matches;
	}
	
	public String toString() {
		String s = "";
		
		System.out.println("Matches length: "+matches.length);
		for (int i=0; i < matches.length; i++) {
			System.out.println(i+" / "+matches.length);
			s = s + matches[i].toString() + "\n";
		}
		
		return s;
	}
}
